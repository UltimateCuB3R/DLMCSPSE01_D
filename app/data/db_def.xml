<?xml version="1.0"?>
<TABLES>
	<TABLE NAME="EXERCISE">
		<COLUMN TYPE="ID">ID</COLUMN>
		<COLUMN TYPE="TEXT">NAME</COLUMN>
		<COLUMN TYPE="TEXT">DESCRIPTION</COLUMN>
		<COLUMN TYPE="REAL">DURATION</COLUMN>
		<!--<COLUMN TYPE="INTEGER" RELATION="CATEGORY">CATEGORY_ID</COLUMN>-->
		<COLUMN TYPE="TEXT">VIDEO_URL</COLUMN>
		<RELATION KEY="EXERCISE_ID">EXERCISE_UNIT</RELATION>
		<RELATION KEY="EXERCISE_ID">EXERCISE_RESOURCE</RELATION>
		<RELATION KEY="EXERCISE_ID">EXERCISE_CATEGORY</RELATION>
	</TABLE>
	<TABLE NAME="EXERCISE_UNIT">
		<COLUMN TYPE="INTEGER" RELATION="EXERCISE">EXERCISE_ID</COLUMN>
		<COLUMN TYPE="INTEGER" RELATION="UNIT">UNIT_ID</COLUMN>
	</TABLE>
	<TABLE NAME="EXERCISE_RESOURCE">
		<COLUMN TYPE="INTEGER" RELATION="EXERCISE">EXERCISE_ID</COLUMN>
		<COLUMN TYPE="INTEGER" RELATION="RESOURCE">RESOURCE_ID</COLUMN>
	</TABLE>
	<TABLE NAME="EXERCISE_CATEGORY">
		<COLUMN TYPE="INTEGER" RELATION="EXERCISE">EXERCISE_ID</COLUMN>
		<COLUMN TYPE="INTEGER" RELATION="CATEGORY">CATEGORY_ID</COLUMN>
	</TABLE>
	<TABLE NAME="UNIT">
		<COLUMN TYPE="ID">ID</COLUMN>
		<COLUMN TYPE="TEXT">NAME</COLUMN>
		<COLUMN TYPE="TEXT">DESCRIPTION</COLUMN>
		<COLUMN TYPE="REAL">DURATION</COLUMN>
		<!--<COLUMN TYPE="INTEGER" RELATION="CATEGORY">CATEGORY_ID</COLUMN>-->
		<RELATION KEY="UNIT_ID">EXERCISE_UNIT</RELATION>
		<RELATION KEY="UNIT_ID">UNIT_PLAN</RELATION>
		<RELATION KEY="UNIT_ID">UNIT_CATEGORY</RELATION>
	</TABLE>
	<TABLE NAME="UNIT_PLAN">
		<COLUMN TYPE="INTEGER" RELATION="UNIT">UNIT_ID</COLUMN>
		<COLUMN TYPE="INTEGER" RELATION="PLAN">PLAN_ID</COLUMN>
	</TABLE>
	<TABLE NAME="UNIT_CATEGORY">
		<COLUMN TYPE="INTEGER" RELATION="UNIT">UNIT_ID</COLUMN>
		<COLUMN TYPE="INTEGER" RELATION="CATEGORY">CATEGORY_ID</COLUMN>
	</TABLE>
	<TABLE NAME="PLAN">
		<COLUMN TYPE="ID">ID</COLUMN>
		<COLUMN TYPE="TEXT">NAME</COLUMN>
		<COLUMN TYPE="TEXT">DESCRIPTION</COLUMN>
		<RELATION KEY="PLAN_ID">UNIT_PLAN</RELATION>
		<RELATION KEY="PLAN_ID">PLAN_CALENDAR</RELATION>
	</TABLE>
	<TABLE NAME="PLAN_CALENDAR">
		<COLUMN TYPE="ID">ID</COLUMN>
		<COLUMN TYPE="INTEGER" RELATION="PLAN">PLAN_ID</COLUMN>
		<COLUMN TYPE="REAL">DATE</COLUMN>
	</TABLE>
	<TABLE NAME="CATEGORY">
		<COLUMN TYPE="ID">ID</COLUMN>
		<COLUMN TYPE="TEXT">NAME</COLUMN>
		<COLUMN TYPE="TEXT">DESCRIPTION</COLUMN>
		<COLUMN TYPE="TEXT">COLOR</COLUMN>
		<RELATION KEY="CATEGORY_ID">EXERCISE_CATEGORY</RELATION>
		<RELATION KEY="CATEGORY_ID">UNIT_CATEGORY</RELATION>
	</TABLE>
	<TABLE NAME="RESOURCE">
		<COLUMN TYPE="ID">ID</COLUMN>
		<COLUMN TYPE="TEXT">NAME</COLUMN>
		<COLUMN TYPE="TEXT">DESCRIPTION</COLUMN>
		<RELATION KEY="RESOURCE_ID">EXERCISE_RESOURCE</RELATION>
	</TABLE>
</TABLES>