<?xml version="1.0"?>
<TABLES>
	<TABLE NAME="EXERCISE">
		<COLUMN TYPE="INTEGER">ID</COLUMN>
		<COLUMN TYPE="TEXT">NAME</COLUMN>
		<COLUMN TYPE="TEXT">DESCRIPTION</COLUMN>
		<COLUMN TYPE="REAL">DURATION</COLUMN>
		<COLUMN TYPE="INTEGER">CATEGORY</COLUMN>
		<COLUMN TYPE="TEXT">VIDEO_URL</COLUMN>
	</TABLE>
	<TABLE NAME="UNIT">
		<COLUMN TYPE="INTEGER">ID</COLUMN>
		<COLUMN TYPE="TEXT">NAME</COLUMN>
		<COLUMN TYPE="TEXT">DESCRIPTION</COLUMN>
		<COLUMN TYPE="REAL">DURATION</COLUMN>
		<COLUMN TYPE="INTEGER">CATEGORY</COLUMN>
		<COLUMN TYPE="INTEGER">EXERCISE_ID</COLUMN>
		<COLUMN TYPE="INTEGER">RESOURCE_ID</COLUMN>
	</TABLE>
	<TABLE NAME="PLAN">
		<COLUMN TYPE="INTEGER">ID</COLUMN>
		<COLUMN TYPE="TEXT">NAME</COLUMN>
		<COLUMN TYPE="TEXT">DESCRIPTION</COLUMN>
		<COLUMN TYPE="INTEGER">UNIT_ID</COLUMN>
